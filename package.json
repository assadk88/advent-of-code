{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "My solutions to Advent of Code",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules npx jest --watch",
    "test:2023": "npm test -- --testPathPattern=/2023",
    "test:2022": "npm test -- --testPathPattern=/2022",
    "lint": "eslint . --cache --ext .js",
    "lint:fix": "eslint . --cache --ext .js --fix"
  },
  "keywords": [
    "advent-of-code",
    "aoc",
    "2022",
    "2023",
    "javascript"
  ],
  "author": "Assad Khan",
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0"
  }
}
